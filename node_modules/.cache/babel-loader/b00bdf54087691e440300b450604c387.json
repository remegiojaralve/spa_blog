A;AAVA,GAtBA;AA8CA,EAAA,QAAA,oBACA,UAAA,CAAA,CAAA,WAAA,CAAA,CADA,CA9CA;AAiDA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,oBACA,KADA,EACA;AACA,WAAA,YAAA,CAAA,KAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,WAAA,UAAA,CAAA,KAAA;AACA;AALA,GAjDA;AAwDA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,WAAA,EAAA,cAAA,CAAA,CADA;AAEA,IAAA,KAFA,mBAEA;AAAA;;AACA,WAAA,EAAA,CAAA,MAAA;;AACA,UAAA,CAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA,aAAA,SAAA,CAAA,KAAA,SAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,UAAA,KAAA,CAAA,QAAA;AACA,SAJA;AAKA,aAAA,SAAA,GAAA;AACA,UAAA,KAAA,EAAA,EADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA;AAIA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AAAA;;AACA,WAAA,YAAA,CAAA,KAAA,YAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA;AACA,OAHA;AAIA,KArBA;AAsBA,IAAA,UAtBA,sBAsBA,KAtBA,EAsBA;AACA,UAAA,KAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AACA,KA5BA;AA6BA,IAAA,WA7BA,yBA6BA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,aAAA,SAAA,GAAA,KAAA;AACA,aAAA,WAAA,GAAA,UAAA;AACA,aAAA,YAAA,GAAA,IAAA;AACA,OAJA,MAIA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,aAAA,WAAA,GAAA,OAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA;AACA;AAvCA;AAxDA,CAAA","sourcesContent":["<template>\n  <div class=\"forms\">\n    <div class=\"forms-container\">\n      <h3 class=\"forms-heading\">{{formHeading}}</h3>\n      <div v-show=\"showLogin\" class=\"forms-input-container\">\n        <form @submit.prevent=\"logIn\">\n          <div class=\"forms-input\">\n            <label>Email</label><br>\n            <input type=\"text\" v-model=\"loginInfo.logEmail\">\n            <span v-if=\"$v.loginInfo.logEmail.required && $v.loginInfo.logEmail.$dirty\">email is required</span>\n          </div>\n          <div class=\"forms-input\">\n            <label>Password</label><br>\n            <input type=\"password\" v-model=\"loginInfo.logPassword\">\n          </div>\n          <input class=\"forms-button\" type=\"submit\" value=\"LOGIN\">\n        </form>\n        <p class=\"forms-text\">No account yet? <span @click=\"toggleForms\">REGISTER HERE</span></p>\n      </div>\n      <div v-show=\"showRegister\" class=\"forms-input-container\">\n        <form @submit.prevent=\"register\">\n          <div class=\"forms-input\">\n            <label>Email</label><br>\n            <input type=\"text\" v-model=\"registerData.regEmail\">\n          </div>\n          <div class=\"forms-input\">\n            <label>Password</label><br>\n            <input type=\"password\" v-model=\"registerData.regPassword\">\n          </div>\n          <div class=\"forms-input\">\n            <label>Confirm Password</label><br>\n            <input type=\"password\" v-model=\"registerData.regConfirmPass\">\n          </div>\n          <input class=\"forms-button\" type=\"submit\" value=\"REGISTER\">\n        </form>\n        <p class=\"forms-text\">No account yet? <span @click=\"toggleForms\">LOGIN HERE</span></p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\nimport { required, minLength, email, sameAs } from 'vuelidate/lib/validators'\n\nexport default {\n  name: 'Forms',\n  props: {\n    hideForm: Function\n  },\n  data () {\n    return {\n      formHeading: 'LOGIN',\n      showLogin: true,\n      showRegister: false,\n      reg: /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/, // eslint-disable-line no-useless-escape\n      loginInfo: {\n        logEmail: '',\n        logPassword: ''\n      },\n      registerData: {\n        regEmail: '',\n        regPassword: '',\n        regCofirmPass: ''\n      }\n    }\n  },\n  validations: {\n    loginInfo: {\n      logEmail: {\n        required,\n        email\n      },\n      logPassword: {\n        required\n      }\n    },\n    registerData: {\n      regEmail: {\n        required,\n        email\n      },\n      regPassword: {\n        required,\n        minLength: minLength(6)\n      },\n      regCofirmPass: {\n        sameAsPassword: sameAs('regPassword')\n      }\n    }\n  },\n  computed: {\n    ...mapGetters(['isLogined'])\n  },\n  watch: {\n    regEmail (value) {\n      this.registerData.email = value\n      console.log(value)\n      this.checkEmail(value)\n    }\n  },\n  methods: {\n    ...mapActions(['loginUser', 'registerUser']),\n    logIn () {\n      this.$v.$touch()\n      if (!this.$v.$invalid) {\n        this.loginUser(this.loginInfo)\n          .then(res => {\n            console.log(res)\n            this.hideForm()\n          })\n        this.loginInfo = {\n          email: '',\n          password: ''\n        }\n      }\n    },\n    register () {\n      this.registerUser(this.registerData)\n        .then(res => {\n          this.toggleForms()\n        })\n    },\n    checkEmail (value) {\n      if (this.reg.test(value)) {\n        console.log('Yes')\n      } else {\n        console.log('No')\n      }\n    },\n    toggleForms () {\n      if (this.showLogin) {\n        this.showLogin = false\n        this.formHeading = 'REGISTER'\n        this.showRegister = true\n      } else {\n        this.showLogin = true\n        this.formHeading = 'LOGIN'\n        this.showRegister = false\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.forms {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-color: white;\n  text-align: left;\n}\n\n.forms-container {\n  width: 460px;\n}\n\n.forms-heading {\n  font-size: 30px;\n  color: black;\n  letter-spacing: 0.1em;\n}\n\n.forms-input-container {\n  margin-top: 25px;\n}\n\n.forms-input {\n  font-size: 18px;\n  letter-spacing: 0.1em;\n  color: black;\n  margin-top: 21px;\n\n  input {\n    width: 100%;\n    height: 60px;\n    font-size: 18px;\n    color: black;\n    padding: 10px;\n    letter-spacing: 0.1em;\n    margin-top: 4px;\n  }\n}\n\n.forms-button {\n  width: 100%;\n  height: 70px;\n  background-color: black;\n  font-size: 20px;\n  letter-spacing: 0.1em;\n  color: white;\n  font-weight: bold;\n  cursor: pointer;\n  border: none;\n  margin-top: 20px;\n}\n\n.forms-text {\n  font-size: 18px;\n  color: black;\n  letter-spacing: 0.1em;\n  text-align: center;\n  margin-top: 36px;\n\n  span {\n    font-weight: bold;\n    cursor: pointer;\n  }\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}