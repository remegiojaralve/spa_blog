AAA,EAAA;AAFA,OAAA;AAIA,KAXA;AAYA;AAEA;AACA,IAAA,UAfA,sBAeA,KAfA,EAeA;AACA,UAAA,KAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AACA,KArBA;AAsBA,IAAA,WAtBA,yBAsBA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,aAAA,SAAA,GAAA,KAAA;AACA,aAAA,WAAA,GAAA,UAAA;AACA,aAAA,YAAA,GAAA,IAAA;AACA,OAJA,MAIA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,aAAA,WAAA,GAAA,OAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA;AACA;AAhCA;AAhCA,CAAA","sourcesContent":["<template>\n  <div class=\"forms\">\n    <div class=\"forms-container\">\n      <h3 class=\"forms-heading\">{{formHeading}}</h3>\n      <div v-show=\"showLogin\" class=\"forms-input-container\">\n        <form @submit.prevent=\"logIn\">\n          <div class=\"forms-input\">\n            <label>Email</label><br>\n            <input type=\"text\" v-model=\"loginInfo.email\">\n          </div>\n          <div class=\"forms-input\">\n            <label>Password</label><br>\n            <input type=\"password\" v-model=\"loginInfo.password\">\n          </div>\n          <input class=\"forms-button\" type=\"submit\" value=\"LOGIN\">\n        </form>\n        <p class=\"forms-text\">No account yet? <span @click=\"toggleForms\">REGISTER HERE</span></p>\n      </div>\n      <div v-show=\"showRegister\" class=\"forms-input-container\">\n        <form>\n          <div class=\"forms-input\">\n            <label>Email</label><br>\n            <input type=\"text\" v-model=\"registerData.email\">\n          </div>\n          <div class=\"forms-input\">\n            <label>Password</label><br>\n            <input type=\"text\" v-model=\"registerData.password\">\n          </div>\n          <div class=\"forms-input\">\n            <label>Confirm Password</label><br>\n            <input type=\"text\" v-model=\"registerData.confirmPass\">\n          </div>\n          <input class=\"forms-button\" type=\"submit\" value=\"REGISTER\">\n        </form>\n        <p class=\"forms-text\">No account yet? <span @click=\"toggleForms\">LOGIN HERE</span></p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\n\nexport default {\n  name: 'Forms',\n  props: {\n    hideForm: Function\n  },\n  data () {\n    return {\n      formHeading: 'LOGIN',\n      showLogin: true,\n      showRegister: false,\n      reg: /^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/, // eslint-disable-line no-useless-escape\n      loginInfo: {\n        email: '',\n        password: ''\n      },\n      registerData: {\n        email: '',\n        password: '',\n        cofirmPass: ''\n      }\n    }\n  },\n  computed: {\n    ...mapGetters(['isLogined'])\n  },\n  watch: {\n    email (value) {\n      this.registerData.email = value\n      console.log(value)\n      this.checkEmail(this.registerData.email)\n    }\n  },\n  methods: {\n    ...mapActions(['loginUser', 'registerUser']),\n    logIn () {\n      this.loginUser(this.loginInfo)\n        .then(res => {\n          this.hideForm()\n        })\n      this.loginInfo = {\n        email: '',\n        password: ''\n      }\n    },\n    // register () {\n\n    // },\n    checkEmail (value) {\n      if (this.reg.test(value)) {\n        console.log('Yes')\n      } else {\n        console.log('No')\n      }\n    },\n    toggleForms () {\n      if (this.showLogin) {\n        this.showLogin = false\n        this.formHeading = 'REGISTER'\n        this.showRegister = true\n      } else {\n        this.showLogin = true\n        this.formHeading = 'LOGIN'\n        this.showRegister = false\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.forms {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-color: white;\n  text-align: left;\n}\n\n.forms-container {\n  width: 460px;\n}\n\n.forms-heading {\n  font-size: 30px;\n  color: black;\n  letter-spacing: 0.1em;\n}\n\n.forms-input-container {\n  margin-top: 25px;\n}\n\n.forms-input {\n  font-size: 18px;\n  letter-spacing: 0.1em;\n  color: black;\n  margin-top: 21px;\n\n  input {\n    width: 100%;\n    height: 60px;\n    font-size: 18px;\n    color: black;\n    padding: 10px;\n    letter-spacing: 0.1em;\n    margin-top: 4px;\n  }\n}\n\n.forms-button {\n  width: 100%;\n  height: 70px;\n  background-color: black;\n  font-size: 20px;\n  letter-spacing: 0.1em;\n  color: white;\n  font-weight: bold;\n  cursor: pointer;\n  border: none;\n  margin-top: 20px;\n}\n\n.forms-text {\n  font-size: 18px;\n  color: black;\n  letter-spacing: 0.1em;\n  text-align: center;\n  margin-top: 36px;\n\n  span {\n    font-weight: bold;\n    cursor: pointer;\n  }\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}