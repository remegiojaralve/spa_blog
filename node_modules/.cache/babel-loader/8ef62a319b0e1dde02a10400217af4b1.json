 blogs...\">\n      <ul>\n        <li v-for=\"( blog, index) in searchPosts\" :key=\"blog.id\">\n          <router-link class=\"post-link\" :to=\"`/posts/${blog.id}`\">\n            <div class=\"post-image\" v-if=\"allPhotos.length > 0\">\n              <img :src=\"allPhotos[index].url\" :alt=\"blog.title\" />\n            </div>\n            <h3> {{blog.title | snippet}} </h3>\n            <!-- <span v-if=\"allUsers.length > 0\"> {{allUsers[randomUser].name}} </span> -->\n          </router-link>\n        </li>\n      </ul>\n    </div>\n</template>\n\n<script>\n\n// import axios from 'axios'\nimport { mapGetters, mapActions } from 'vuex'\n\nexport default {\n  name: 'BlogList',\n  data () {\n    return {\n      user: '',\n      photo: '',\n      search: ''\n    }\n  },\n  created () {\n    this.fetchPhotos()\n    this.fetchPosts()\n    this.fetchUsers()\n  },\n  methods: {\n    ...mapActions(['fetchPosts', 'fetchPhotos', 'fetchUsers'])\n  },\n  computed: {\n    ...mapGetters(['allPosts', 'allPhotos', 'allUsers']),\n    searchPosts () {\n      return this.allPosts.filter((post) => {\n        return post.title.match(this.search)\n      })\n    }\n  },\n  filters: {\n    snippet (value) {\n      if (value.length < 25) {\n        return value\n      }\n      return value.slice(0, 25) + '...'\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .post-list {\n    width: 100%;\n    margin: auto;\n  }\n\n  .post-link {\n    text-decoration: none;\n    color: black;\n    display: block;\n    width: 100%;\n    height: 100%;\n  }\n\n  .post-image {\n    width: 100%;\n    height: 250px;\n    overflow: hidden;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n\n  ul {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n  }\n\n  li {\n    list-style: none;\n    width: 30%;\n    border: 1px solid #eee;\n    margin-top: 20px;\n    padding: 10px;\n\n    &:hover {\n      img {\n        opacity: 0.5;\n      }\n\n      h3 {\n        color: blue;\n      }\n    }\n  }\n\n  img {\n    width: 100%;\n    transition: opacity 0.3s ease-out;\n  }\n\n  h3 {\n    transition: color 0.3s ease-out;\n  }\n\n  p {\n    text-align: left;\n  }\n</style>\n"],"sourceRoot":"src/components"}]}